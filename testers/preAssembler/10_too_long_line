; ----------- test 11 -----------
; line length is more than 80 characters
; output: *.am file
;         *error msg about line 7,8

mov r2, r3                                                                       extra text after 80+1 chars
LABEL: cmp abc, r4
       add LABEL, %@
.extern LABEL

mcr MACRO
    sub r2, r3                                                                      extra text after 80+1 chars
endmcr

jmp LABEL